<div id="extract-content" class="col-xs-12 annotatable" data-annotate="word">
	<% @document.content.css("text p").each do |paragraph| %>
		<div class="paragraph">
			<% paragraph.css("s").each do |sentence| %>
				<div class="sentence">
					<% sentence.css("w").each do |word| %>
						<div id="<%= word["id"] %>" class="word <%= "no-space" if word["pws"].eql?("no") %>"><%= word.content %></div>
					<% end %>
				</div>
			<% end %>
		</div>
	<% end %>
</div>
